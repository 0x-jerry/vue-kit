{
  "name": "@0x-jerry/vue-kit",
  "description": "Vue component kit.",
  "version": "1.6.4",
  "main": "./dist/vue-kit.es.js",
  "types": "./dist/types/lib.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0x-jerry/vue-kit.git"
  },
  "exports": {
    ".": {
      "default": "./dist/vue-kit.es.js"
    },
    "./resolver": {
      "default": "./dist/resolver.js"
    },
    "./*": "./*"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/0x-jerry/vue-kit/issues"
  },
  "homepage": "https://github.com/0x-jerry/vue-kit#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build:component && npm run build:dts && npm run build:resolver",
    "build:docs": "vite build",
    "build:component": "vite build -c vite.config.build.ts",
    "build:resolver": "tsup",
    "build:dts": "vue-tsc -d --emitDeclarationOnly --outDir dist/types -p tsconfig.build.json",
    "test": "vue-tsc --noEmit",
    "serve": "vite preview",
    "release": "x-release"
  },
  "tsup": {
    "entry": {
      "resolver": "resolver/index.ts"
    },
    "splitting": false,
    "sourcemap": true,
    "dts": true
  },
  "dependencies": {
    "@0x-jerry/utils": "^1.8.2",
    "@floating-ui/dom": "^1.0.1",
    "@unocss/reset": "^0.45.20",
    "@vueuse/core": "^9.2.0",
    "cropperjs": "^1.5.12",
    "normalize.css": "^8.0.1",
    "qrcode": "^1.5.1",
    "vue": "^3.2.39"
  },
  "devDependencies": {
    "@0x-jerry/x-release": "^0.3.6",
    "@iconify/json": "^2.1.105",
    "@types/node": "^18.7.16",
    "@types/qrcode": "^1.5.0",
    "@unocss/transformer-directives": "^0.45.20",
    "@vitejs/plugin-vue": "^3.1.0",
    "less": "^4.1.3",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3",
    "unocss": "^0.45.20",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-icons": "^0.14.9",
    "unplugin-vue-components": "^0.22.6",
    "vite": "^3.1.0",
    "vite-plugin-pages": "^0.26.0",
    "vue-router": "^4.1.5",
    "vue-tsc": "^0.40.13"
  }
}