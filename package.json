{
  "name": "@0x-jerry/vue-kit",
  "description": "Vue kit",
  "version": "2.3.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0x-jerry/vue-kit.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/0x-jerry/vue-kit/issues"
  },
  "homepage": "https://github.com/0x-jerry/vue-kit#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/components/*.d.ts",
      "default": "./dist/components/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "play": "npm -C playground run dev",
    "test": "vitest",
    "build": "vite build",
    "prepublishOnly": "npm run build",
    "lint": "biome lint src/**/* --write",
    "release": "x-release",
    "ci:test": "vitest run"
  },
  "dependencies": {
    "@vueuse/core": "^12.5.0",
    "type-fest": "^4.33.0",
    "vue": "^3.5.13",
    "vue-component-type-helpers": "^2.2.0"
  },
  "devDependencies": {
    "@0x-jerry/utils": "^4.2.2",
    "@0x-jerry/x-release": "^2.2.2",
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.13.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/coverage-v8": "^3.0.5",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "happy-dom": "^16.8.1",
    "jsdom": "^26.0.0",
    "less": "^4.2.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "unocss": "^65.4.3",
    "vite": "^6.0.11",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.0.5"
  }
}