{
  "name": "@0x-jerry/vue-kit",
  "description": "Vue kit",
  "version": "2.3.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0x-jerry/vue-kit.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/0x-jerry/vue-kit/issues"
  },
  "homepage": "https://github.com/0x-jerry/vue-kit#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/components/*.d.ts",
      "default": "./dist/components/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "play": "npm -C playground run dev",
    "test": "vitest",
    "build": "vite build",
    "prepublishOnly": "npm run build",
    "lint": "biome check --write",
    "release": "x-release",
    "ci:test": "vitest run"
  },
  "dependencies": {
    "@standard-schema/spec": "^1.1.0",
    "@vueuse/core": "^14.1.0",
    "type-fest": "^5.4.1",
    "vue": "^3.5.26",
    "vue-component-type-helpers": "^3.2.2"
  },
  "devDependencies": {
    "@0x-jerry/utils": "^6.0.0",
    "@0x-jerry/x-release": "^2.4.1",
    "@biomejs/biome": "^2.3.11",
    "@types/node": "^25.0.8",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.3",
    "@vitest/coverage-v8": "^4.0.17",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "happy-dom": "^20.3.0",
    "jsdom": "^27.4.0",
    "less": "^4.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "unocss": "66.6.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.17",
    "zod": "^4.3.5"
  }
}
