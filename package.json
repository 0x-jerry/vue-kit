{
  "name": "@0x-jerry/vue-kit",
  "description": "Vue component kit.",
  "version": "1.1.6",
  "main": "./dist/vue-kit.es.js",
  "types": "./dist/types/lib.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0x-jerry/vue-kit.git"
  },
  "exports": {
    ".": {
      "default": "./dist/vue-kit.es.js"
    },
    "./resolver": {
      "default": "./dist/resolver.js"
    },
    "./*": "./*"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/0x-jerry/vue-kit/issues"
  },
  "homepage": "https://github.com/0x-jerry/vue-kit#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build:component && npm run build:dts && npm run build:resolver",
    "build:docs": "vite build",
    "build:component": "vite build -c vite.config.build.ts",
    "build:resolver": "tsup",
    "build:dts": "vue-tsc -d --emitDeclarationOnly --outDir dist/types -p tsconfig.build.json",
    "test": "vue-tsc --noEmit",
    "serve": "vite preview",
    "release": "x-release"
  },
  "tsup": {
    "entry": {
      "resolver": "resolver/index.ts"
    },
    "splitting": false,
    "sourcemap": true,
    "dts": true
  },
  "dependencies": {
    "@0x-jerry/utils": "^1.5.0",
    "@vueuse/core": "^8.2.6",
    "cropperjs": "^1.5.12",
    "qrcode": "^1.5.0",
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@0x-jerry/x-release": "^0.3.2",
    "@iconify-json/carbon": "^1.1.3",
    "@iconify-json/mdi": "^1.1.9",
    "@types/node": "^17.0.24",
    "@types/qrcode": "^1.4.2",
    "@vitejs/plugin-vue": "^2.3.1",
    "less": "^4.1.2",
    "tsup": "^5.12.5",
    "typescript": "^4.6.3",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-icons": "^0.14.1",
    "unplugin-vue-components": "^0.19.3",
    "vite": "^2.9.5",
    "vite-plugin-optimize-persist": "^0.1.2",
    "vite-plugin-package-config": "^0.1.1",
    "vite-plugin-pages": "^0.22.0",
    "vite-plugin-windicss": "^1.8.4",
    "vue-router": "^4.0.14",
    "vue-tsc": "^0.34.6",
    "windicss": "^3.5.1"
  },
  "vite": {
    "optimizeDeps": {
      "include": [
        "@0x-jerry/utils",
        "@vueuse/core",
        "cropperjs",
        "vue",
        "vue-router"
      ]
    }
  }
}