{
  "name": "@0x-jerry/vue-kit",
  "description": "Vue component kit.",
  "version": "1.7.0",
  "main": "./dist/vue-kit.js",
  "types": "./dist/types/lib.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0x-jerry/vue-kit.git"
  },
  "exports": {
    ".": {
      "default": "./dist/vue-kit.js"
    },
    "./resolver": {
      "default": "./dist/resolver.cjs"
    },
    "./*": "./*"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/0x-jerry/vue-kit/issues"
  },
  "homepage": "https://github.com/0x-jerry/vue-kit#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build:component && npm run build:dts && npm run build:resolver",
    "build:docs": "vite build",
    "build:component": "vite build -c vite.config.build.ts",
    "build:resolver": "tsup",
    "build:dts": "vue-tsc -d --emitDeclarationOnly --outDir dist/types -p tsconfig.build.json",
    "g:css": "tsx src/theme/generate.ts",
    "lint": "vue-tsc --noEmit",
    "serve": "vite preview",
    "release": "x-release"
  },
  "tsup": {
    "entry": {
      "resolver": "resolver/index.ts"
    },
    "splitting": false,
    "sourcemap": true,
    "dts": true
  },
  "dependencies": {
    "@0x-jerry/utils": "^1.18.0",
    "@floating-ui/dom": "^1.4.5",
    "@unocss/reset": "^0.53.5",
    "@vueuse/core": "^10.2.1",
    "animejs": "^3.2.1",
    "cropperjs": "^1.5.13",
    "normalize.css": "^8.0.1",
    "qrcode": "^1.5.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@0x-jerry/x-release": "^0.3.8",
    "@iconify/json": "^2.2.91",
    "@types/animejs": "^3.1.7",
    "@types/node": "^20.4.2",
    "@types/qrcode": "^1.5.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.4.0",
    "happy-dom": "^10.5.1",
    "jsdom": "^22.1.0",
    "less": "^4.1.3",
    "mock-socket": "^9.2.1",
    "tsup": "^7.1.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "unocss": "^0.55.0",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-icons": "^0.16.5",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.4",
    "vite-plugin-pages": "^0.31.0",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.5"
  }
}