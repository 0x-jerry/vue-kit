{
  "name": "@0x-jerry/vue-kit",
  "description": "Vue kit",
  "version": "2.3.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0x-jerry/vue-kit.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/0x-jerry/vue-kit/issues"
  },
  "homepage": "https://github.com/0x-jerry/vue-kit#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/components/*.d.ts",
      "default": "./dist/components/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "play": "npm -C playground run dev",
    "test": "vitest",
    "build": "vite build",
    "prepublishOnly": "npm run build",
    "lint": "biome lint src/**/* --write",
    "release": "x-release",
    "ci:test": "vitest run"
  },
  "dependencies": {
    "@standard-schema/spec": "^1.0.0",
    "@vueuse/core": "^13.7.0",
    "type-fest": "^4.41.0",
    "vue": "^3.5.19",
    "vue-component-type-helpers": "^3.0.6"
  },
  "devDependencies": {
    "@0x-jerry/utils": "^5.0.0",
    "@0x-jerry/x-release": "^2.4.0",
    "@biomejs/biome": "^2.2.0",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "less": "^4.4.1",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "unocss": "66.4.2",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "zod": "^4.0.17"
  }
}